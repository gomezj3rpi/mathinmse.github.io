    "def rY(angle):\n",
    "    sb = sp.sin(angle)\n",
    "    cb = sp.cos(angle)\n",
    "    M = sp.Matrix([[cb, 0, -sb],\n",
    "                  [0, 1, 0],\n",
    "                  [sb, 0, cb]])\n",
    "    return M\n"
